from "ecc/decaf377" import proofOfOwnership;


// Code to create test cases:
// https://github.com/Zokrates/pycrypto
def testOwnershipTrue() -> bool {
	field[2] pk = [6004179735632513519625614087523760558502557256563490600601285660914264757493, 1466848481773022623446363374457016099083555409649416885995551656349566170536];
	field sk = 3052655993692735480232781265650109101828271430100647642871814177512148714994;
    
	bool out = proofOfOwnership(pk, sk);

	assert(out);
	return true;
}

def testOwnershipFalse() -> bool {
	field[2] pk = [6004179735632513519625614087523760558502557256563490600601285660914264757493, 1466848481773022623446363374457016099083555409649416885995551656349566170536];
	field sk = 1571519858352981955471870925088694351565766824953502245185888024807356683799;
	
	bool out = proofOfOwnership(pk, sk);

	assert(!out);
	return true;
}

def main() {
	assert(testOwnershipTrue());
	assert(testOwnershipFalse());
}